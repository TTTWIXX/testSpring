<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.testtest.practice01.repository.BoardMapper">

    <select id="findAll" resultMap="board">
        SELECT
        board_no, title,
        content, view_count, reg_date_time
        FROM tbl_board
    </select>

    <insert id="save">
        INSERT INTO tbl_board(title,content) VALUES(#{},#{})

    </insert>


</mapper>